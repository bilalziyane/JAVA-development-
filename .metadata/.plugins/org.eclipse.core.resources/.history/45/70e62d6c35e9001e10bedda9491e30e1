import java.sql.Connection;
import java.sql.DriverManager;
import java.sql.SQLException;

public class One {
    public static void main(String[] args) {
        // Database URL
        String url = "jdbc:sqlserver://Bilal\\SQLEXPRESS;databaseName=VENTES;integratedSecurity=true;encrypt=false;trustServerCertificate=false";

        // SQL query to execute
        String query = "SELECT * FROM CLIENT";

        try {
            // Establish connection
            try(Connection connection = DriverManager.getConnection(url)){
                System.out.println("Connection established");
            } catch(SQLException e) {
                System.out.println("Error de connection: " + e.getMessage());
            }

            // Continue with your database operations...
        } catch (Exception e) {
            e.printStackTrace();
        }
    }
}





